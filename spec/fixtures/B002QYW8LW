             <div id="actionPanelOfferSelector_feature_div" class="feature" data-feature-name="actionPanelOfferSelector">
         
     




    

    
    
    
        
            
                
	                
	                
	                	




    
    





































































    
    
    
    
    
    
    
    
    
    
    

    
    
    
    
    
    
    
    

    
    
    

    <div id="apos" data-apstate="onetime" class="a-section a-spacing-none">
        
    </div>

    <script type="text/javascript">
        P.when('jQuery','action-panel').execute(function($, ap) {
            var state = $("#apos").data("apstate");
            var curState = {mode : state};

            ap.setPanelState(curState);
        });
    
        P.when('toggleBB').execute(function(toggleBB) {
            toggleBB.init();
        });
    
        P.when('usedBuyBoxDPv2').execute(function(ubbDPv2){
    	    ubbDPv2.init();
        });
    </script>


	                
                
            
        
        
        
        
        
        
        
    
    
    

    
















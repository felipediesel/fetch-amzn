                        <div id="bRecPricePara" class="bRecPricePara bRecCenteredText">
                            <span id="bRecPriceLabel" class="a-color-secondary"></span>
                            <span id="bRecPrice">$22.74</span>
                        </div>
                        <div class="a-button-stack">
                            <span id="bRecAddToCart" class="a-button a-spacing-small a-button-primary"><span class="a-button-inner"><input id="bRecAddToCartNative" name="submit.addToCart" class="a-button-input" type="submit" value="addToCart" aria-labelledby="bRecAddToCart-announce"><span id="bRecAddToCart-announce" class="a-button-text" aria-hidden="true">Add to Shopping Cart</span></span></span>
        
                            <span id="bRecAddToList" class="a-button"><span class="a-button-inner"><input id="bRecAddToListNative" name="submit.add-to-registry.wishlist" class="a-button-input" type="submit" aria-labelledby="bRecAddToList-announce"><span id="bRecAddToList-announce" class="a-button-text" aria-hidden="true">Add to My List</span></span></span>
